  "scripts": {
    "build": "babel src --out-dir dist",
    "build-example": "babel example/src --out-dir example/dist && cp -r example/src/*.{html,mp3} example/dist",
    "test": "eslint src && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesseanwright/web-audio-x.git"
  },
