{
  "name": "wax-core",
  "version": "0.0.1",
  "description": "A component-driven library to abstract the Web Audio API, intended to be targeted by JSX",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "build-example": "mkdir -p example/dist && rm -rf example/dist/* && rollup -c && cp -r example/src/*.{html,mp3} example/dist",
    "dev": "npm run build && npm run build-example && node example/devServer",
    "test": "eslint --ext js --ext jsx src example/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesseanwright/web-audio-x.git"
  },
  "keywords": [
    "web",
    "audio",
    "api",
    "components",
    "jsx",
    "react"
  ],
  "author": "James Wright <james@jamesswright.co.uk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamesseanwright/web-audio-x/issues"
  },
  "homepage": "https://github.com/jamesseanwright/web-audio-x#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-react-jsx": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "eslint": "5.5.0",
    "eslint-plugin-react": "7.11.1",
    "node-static": "0.7.10",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "rollup": "0.65.0",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-babel": "4.0.2",
    "rollup-plugin-commonjs": "9.1.6",
    "rollup-plugin-node-resolve": "3.3.0"
  }
}
