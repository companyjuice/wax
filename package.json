  "author": "James Wright <james@jamesswright.co.uk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamesseanwright/web-audio-x/issues"
  },
  "homepage": "https://github.com/jamesseanwright/web-audio-x#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/plugin-transform-react-jsx": "7.0.0",
    "eslint": "5.5.0",
    "eslint-plugin-react": "7.11.1"
  }
}
